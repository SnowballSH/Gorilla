let fib = fn(n) {
    let a = 0
    let b = 1

    if n < 0 {
        null
    } else {
        if n <= 1 {
            n
        } else {
            let rep = fn(k, x, y) {
                if k == n - 1 {
                    y
                } else {
                    rep(k + 1, y, x + y)
                }
            }
            rep(0, a, b)
        }
    }
}

display(fib(9))
